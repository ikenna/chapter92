          "event": [
            {
              "listen": "test",
              "script": {
                "id": "b9d314fb-2bd3-4245-b605-7e52c696869e",
                "type": "text/javascript",
                "exec": [
                  "// Validate status 2xx \npm.test(\"[GET]::/v1/catalog/products/:id - Status code is 2xx\", function () {\n   pm.response.to.be.success;\n});\n",
                  "// Response Validation\nconst schema = {\"title\":\"Product\",\"required\":[\"id\",\"name\",\"description\",\"price\",\"keywords\",\"reviewRating\",\"numberOfReviews\"],\"type\":\"object\",\"additionalProperties\":false,\"properties\":{\"id\":{\"type\":\"string\",\"format\":\"uuid\",\"description\":\"Identifier for the product.\",\"example\":\"dcd53ddb-8104-4e48-8cc0-5df1088c6113\"},\"name\":{\"maxLength\":50,\"minLength\":1,\"type\":\"string\",\"description\":\"Name of the product.\",\"example\":\"Acme Uber Dog Rope Toy\"},\"description\":{\"maxLength\":300,\"minLength\":0,\"type\":\"string\",\"description\":\"A description of the product.\",\"example\":\"Acme Uber Dog Rope Toy provides hours of fun for your dog. Great for bouncing, throwing and catching.\"},\"price\":{\"maximum\":1000000,\"minimum\":0,\"type\":\"number\",\"description\":\"Price of the product.\",\"example\":100},\"keywords\":{\"maxItems\":20,\"minItems\":0,\"uniqueItems\":true,\"type\":\"array\",\"items\":{\"type\":\"string\"},\"description\":\"A colleciton of words that describe the product, used for searching for it.\"},\"reviewRating\":{\"maximum\":5,\"minimum\":1,\"type\":\"integer\",\"description\":\"Average product review rating.\",\"format\":\"int32\",\"example\":5},\"numberOfReviews\":{\"type\":\"integer\",\"description\":\"Number of product reviews.\",\"format\":\"int32\",\"example\":10}},\"description\":\"An item in Acme's pet supplies catalog.\"}\n\n// Validate if response matches JSON schema \npm.test(\"[GET]::/v1/catalog/products/:id - Schema is valid\", function() {\n    pm.response.to.have.jsonSchema(schema,{unknownFormats: [\"int32\", \"int64\", \"float\", \"double\"]});\n});\n",
                  "// Validate if response header has matching content-type\npm.test(\"[GET]::/v1/catalog/products/:id - Content-Type is application/json\", function () {\n   pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");\n});\n",
                  "// Validate if response has JSON Body \npm.test(\"[GET]::/v1/catalog/products/:id - Response has JSON Body\", function () {\n    pm.response.to.have.jsonBody();\n});\n"
                ]
              }
            }
